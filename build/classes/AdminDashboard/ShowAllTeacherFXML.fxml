<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox stylesheets="@../themes.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="AdminDashboard.ShowAllTeacherFXMLController">
   <children>
      <HBox alignment="CENTER" prefHeight="20.0" prefWidth="600.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Teacher list" HBox.hgrow="ALWAYS">
               <font>
                  <Font size="22.0" />
               </font>
            </Text>
         </children>
      </HBox>
      <TableView fx:id="TableV" editable="true" prefHeight="806.0" prefWidth="602.0" stylesheets="@../themes.css" VBox.vgrow="ALWAYS">
        <columns>
          <TableColumn fx:id="Id" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="Name" prefWidth="75.0" text="Name" />
            <TableColumn fx:id="Age" prefWidth="75.0" text="Age" />
            <TableColumn fx:id="Email" prefWidth="75.0" text="Email" />
            <TableColumn fx:id="Sex" prefWidth="75.0" text="Sex" />
            <TableColumn fx:id="Phone" prefWidth="75.0" text="Phone" />
            <TableColumn fx:id="Subject" prefWidth="75.0" text="Subject" />
            <TableColumn fx:id="Address" prefWidth="75.0" text="Address" />
        </columns>
         <VBox.margin>
            <Insets top="16.0" />
         </VBox.margin>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
   <styleClass>
      <String fx:value="dot" />
      <String fx:value="menu-down-arrow" />
   </styleClass>
</VBox>
